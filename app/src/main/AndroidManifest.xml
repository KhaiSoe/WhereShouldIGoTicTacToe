<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.pursuit.tictactoe">

    <application
            android:allowBackup="true"
            android:icon="@drawable/x"
            android:label="@string/app_name"
            android:roundIcon="@drawable/x"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".main.AutoPlayActivity">
        </activity>
        <activity android:name=".results.TieActivity">
        </activity>
        <activity android:name=".RulesActivity">
        </activity>
        <activity android:name=".results.Result2Activity">
        </activity>
        <activity android:name=".results.ResultActivity">
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".main.MainActivity">
        </activity>
    </application>

</manifest>